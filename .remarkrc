{
  "settings": {
    "emphasis": "*",
    "strong": "*"
  },
  "plugins": [
    "remark-preset-lint-recommended",
    "remark-preset-lint-markdown-style-guide",
    "remark-validate-links",
    [
      "remark-lint-list-item-indent",
      false
    ],
    ["remark-lint-maximum-line-length", 1000]
  ]
}
